server {
        listen 80 default_server;
        listen [::]:80 default_server;
        server_name _;
        root /var/www/html;
        location / {
                try_files $uri $uri/ =404;
        }
}
server {
	listen 8080;
	listen [::]:8080;
	server_name nginx-filehost;
	location = /stub_status {
    		stub_status;
	}
}